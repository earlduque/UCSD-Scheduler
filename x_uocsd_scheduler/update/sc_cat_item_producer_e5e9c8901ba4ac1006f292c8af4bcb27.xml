	timeslot_gr.setValue('request', current.getUniqueValue());
	timeslot_gr.setValue('start', producer.scheduler_dates_and_times.getRow(i).start);
	var gdt = new GlideDateTime(producer.scheduler_dates_and_times.getRow(i).start);
	var gt = new GlideTime();
	gt.setValue(producer.scheduler_dates_and_times.getRow(i).duration.split(' ')[1]);
	gdt.add(gt);
	timeslot_gr.setValue('end', gdt.getValue());
	timeslot_gr.setValue('short_description', producer.short_description);
	timeslot_gr.setValue('description', producer.description);
	timeslot_gr.setValue('submitter', producer.submitter);
	timeslot_gr.insert();
}

